<ContentDialog
    x:Class="Retrobox.ManageLibraryView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Library">
    <ItemsControl Items="{Binding Libraries}">
        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <ToggleSwitch IsChecked="{Binding IsOn}" />
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>
</ContentDialog>
